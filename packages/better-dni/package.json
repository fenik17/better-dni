{
  "name": "better-dni",
  "version": "4.4.2",
  "description": "The fastest Spanish DNI (NIE / NIF) validation out there.",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Nahuel Scotti <nahuel.scotti@gmail.com> (https://www.singuerinc.com)",
  "repository": "singuerinc/better-dni",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "benchmark": "ts-node -O '{\"module\":\"commonjs\"}' benchmark.ts",
    "build": "rollup -c",
    "dev": "tsc -w",
    "format": "prettier --write \"./src/**/*.{ts,tsx,html}\"",
    "playground": "ts-node -O '{\"module\":\"commonjs\"}' playground.ts",
    "size": "size-limit",
    "test": "jest",
    "prepublish": "cp ../../README.md ./"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@semantic-release/git": "^9.0.0",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@willowi/validate-nif": "^1.0.0",
    "coveralls": "^3.1.0",
    "dni-js": "^0.2.0",
    "dni-js-validator": "^1.0.3",
    "eslint": "^7.27.0",
    "jest": "^27.0.3",
    "nanobench": "^2.1.1",
    "prettier": "^2.3.0",
    "rollup": "^2.50.4",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.3",
    "size-limit": "^4.11.0",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "size-limit": [
    {
      "limit": "1 kB",
      "path": "dist/index.js"
    }
  ],
  "keywords": [
    "dni",
    "nie",
    "nif",
    "spain",
    "espa√±a"
  ],
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
